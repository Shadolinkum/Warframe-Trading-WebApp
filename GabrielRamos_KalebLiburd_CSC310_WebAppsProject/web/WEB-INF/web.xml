<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletCookie</servlet-name>
        <servlet-class>servlet_GR.ServletCookie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletHttpSession</servlet-name>
        <servlet-class>servlet_GR.ServletHttpSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletResponder</servlet-name>
        <servlet-class>servlet_GR.ServletResponder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSendRedirect</servlet-name>
        <servlet-class>servlet_GR.ServletSendRedirect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WTBCategoryListServlet</servlet-name>
        <servlet-class>servlet_GR.WTBCategoryListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuyingServlet</servlet-name>
        <servlet-class>servlet_GR.BuyingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoughtServlet</servlet-name>
        <servlet-class>servlet_GR.BoughtServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionBuy</servlet-name>
        <servlet-class>servlet_GR.TransactionBuy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TradeServlet</servlet-name>
        <servlet-class>servlet_KL.TradeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCookie</servlet-name>
        <url-pattern>/ServletCookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletHttpSession</servlet-name>
        <url-pattern>/ServletHttpSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletResponder</servlet-name>
        <url-pattern>/ServletResponder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSendRedirect</servlet-name>
        <url-pattern>/ServletSendRedirect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WTBCategoryListServlet</servlet-name>
        <url-pattern>/WTBCategoryListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuyingServlet</servlet-name>
        <url-pattern>/BuyingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoughtServlet</servlet-name>
        <url-pattern>/BoughtServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionBuy</servlet-name>
        <url-pattern>/TransactionBuy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TradeServlet</servlet-name>
        <url-pattern>/TradeServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
